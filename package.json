{
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build",
    "dev": "APP_ENV=development next dev",
    "build:development": "APP_ENV=development next build",
    "start:development": "APP_ENV=development next start",
    "preview:development": "npm run build:development && npm run start:development",
    "dev:staging": "APP_ENV=staging next dev",
    "build:staging": "APP_ENV=staging next build",
    "start:staging": "APP_ENV=staging next start",
    "preview:staging": "npm run build:staging && npm run start:staging",
    "dev:production": "APP_ENV=production next dev",
    "build:production": "APP_ENV=production next build",
    "start:production": "APP_ENV=production next start",
    "preview:production": "npm run build:production && npm run start:production",
    "type-check": "tsc",
    "component": "plop component --plopfile ./plop/plopfile.js"
  },
  "prettier": "@airnauts/style-guide/prettier",
  "dependencies": {
    "next": "^13.0.7",
    "react-dom": "^18.2.0",
    "react": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@airnauts/style-guide": "^1.5.1",
    "@types/node": "^12.12.21",
    "@types/react-dom": "^18.0.9",
    "@types/react": "^18.0.26",
    "eslint": "^8.28.0",
    "plop": "^3.1.1",
    "prettier": "^2.8.1",
    "stylelint": "^14.16.0"
  }
}
